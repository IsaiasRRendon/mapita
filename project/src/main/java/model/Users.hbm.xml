<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.Users" table="users" schema="public" catalog="mapita">
        <id name="userId">
            <column name="user_id" sql-type="integer"/>
        </id>
        <property name="userName">
            <column name="user_name" sql-type="text"/>
        </property>
        <property name="userEmail">
            <column name="user_email" sql-type="text"/>
        </property>
        <property name="userPassword">
            <column name="user_password" sql-type="text"/>
        </property>
        <property name="userRole">
            <column name="user_role" sql-type="user_role"/>
        </property>
    </class>
</hibernate-mapping>